---
import { Icon } from "astro-icon/components";
import Version from "@/components/Version.vue";
---

<div id="container">
    <Icon id="background" name="background" />
    <main>
        <section>
            <Version class="text-2xl" client:only="vue" />
        </section>
    </main>
</div>

<style>
    #background {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: -1;
        filter: blur(100px);
    }
</style>
